{
  "name": "thinking-tools",
  "description": "A LangGraph-based system for selecting appropriate thinking tools",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "tsc --noEmit && eslint . --fix",
    "lint:check": "tsc --noEmit && eslint .",
    "prepare": "husky",
    "start": "bun run src/index.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "test:jest": "jest",
    "test:jest:coverage": "jest --coverage --passWithNoTests",
    "test:jest:watch": "jest --watch",
    "commit:generate": "bun run src/scripts/generate-commit-message.ts"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.11",
    "@langchain/community": "^0.3.11",
    "@langchain/core": "^0.3.24",
    "@langchain/langgraph": "^0.0.6",
    "@langchain/openai": "^0.0.14",
    "langchain": "^0.1.21"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "~9.17.0",
    "husky": "~9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "~15.2.11",
    "prettier": "~3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "typescript": "~5.7.2"
  },
  "license": "MIT"
}
